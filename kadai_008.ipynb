        "#ランダムな整数を利用するために、randomモジュールをインポート\n",
        "import random\n",
        "var = random.randint(0,100)\n",
        "if var % 3 == 0 and var % 5 == 0 : #余りを表す演算子は%\n",
        "  print(\"FizzBuzz\")\n",
        "elif var % 3 == 0 :\n",
        "  print(\"Fizz\")\n",
        "elif var % 5 == 0 :\n",
        "  print(\"Buzz\")\n",
        "else :\n",
        "   print(var)"
